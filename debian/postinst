#!/bin/sh

JNCCLIENT_HOME=/usr/share/jncclient

chown root:root $JNCCLIENT_HOME/ncsvc
chown root:root /etc/default/jncclient
chown -R root:root /etc/jncclient
chown -R root:root $JNCCLIENT_HOME/ncsvc-wrapper.so
chmod 755 $JNCCLIENT_HOME/ncsvc
chmod 755 $JNCCLIENT_HOME/ncdiag
chmod 755 $JNCCLIENT_HOME/getx509certificate.sh
chmod 600 /etc/default/jncclient
